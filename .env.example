# MyUrls 应用配置
MYURLS_APP_ENVIRONMENT=production

# 服务器配置
MYURLS_SERVER_PORT=8080
MYURLS_SERVER_DOMAIN=localhost:8080
MYURLS_SERVER_PROTO=https

# 存储配置
MYURLS_STORAGE_TYPE=redis
MYURLS_STORAGE_REDIS_ADDR=localhost:6379
MYURLS_STORAGE_REDIS_PASSWORD=
MYURLS_STORAGE_SQLITE_FILE=./data/myurls.db
MYURLS_STORAGE_CACHE_SIZE=128
MYURLS_STORAGE_CACHE_TTL=300

# 日志配置
MYURLS_LOG_LEVEL=info
MYURLS_LOG_FORMAT=text
MYURLS_LOG_OUTPUT=stdout
